clean:
	@-rm -rf $(R3LIB) r3-core-host$(BIN_SUFFIX) r3-view-host$(BIN_SUFFIX) r3-core$(BIN_SUFFIX) r3-view$(BIN_SUFFIX) $(OBJ_DIR)/ 
	@-rm -fr $(GENS)
	@-rm prep-lib-stamp prep-core-stamp prep-view-stamp

GENS = $I/tmp-funcargs.h \
	   $I/tmp-funcs.h \
	   $I/tmp-strings.h \
	   $R/b-boot.c \
	   $I/tmp-boot.h \
	   $I/tmp-bootdefs.h \
	   $I/tmp-comptypes.h \
	   $I/tmp-errnums.h \
	   $I/tmp-evaltypes.h \
	   $I/tmp-exttypes.h \
	   $I/tmp-maketypes.h \
	   $I/tmp-portmodes.h \
	   $I/tmp-sysctx.h \
	   $I/tmp-sysobj.h \
	   $I/host-lib.h \
	   $I/reb-lib.h \
	   $I/reb-lib-lib.h \
	   $I/host-init.h

